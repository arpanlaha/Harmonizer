{
  "build": {
    "env": {
      "GATSBY_TELEMETRY_DISABLED": "1",
      "BACKEND_URL": "@harmonizer_backend_url"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/client/logo.svg"
    },
    {
      "src": "^((?!robots.txt).)*$",
      "status": 308,
      "headers": { "Location": "https://arpanlaha.com/page-not-found" }
    }
  ]
}
